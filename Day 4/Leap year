#include <stdio.h>

int main() {
    int year;

    printf("Enter a year: ");
    scanf("%d", &year);

    // Basic Logic: Divisible by 400? OR (Divisible by 4 AND NOT divisible by 100)
    if (year % 400 == 0) {
        printf("%d is a LEAP YEAR.\n", year);
    } else if (year % 100 == 0) {
        printf("%d is NOT a leap year (it is a century year not divisible by 400).\n", year);
    } else if (year % 4 == 0) {
        printf("%d is a LEAP YEAR.\n", year);
    } else {
        printf("%d is NOT a leap year.\n", year);
    }

    return 0;
}
