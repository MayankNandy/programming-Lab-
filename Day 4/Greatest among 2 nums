#include <stdio.h>

int main() {
    int n_one, n_two;

    printf("Enter first number: ");
    scanf("%d", &n_one);
    printf("Enter second number: ");
    scanf("%d", &n_two);

    if (n_one > n_two) {
        printf("The greatest number is: %d\n", n_one);
    } else if (n_two > n_one) {
        printf("The greatest number is: %d\n", n_two);
    } else {
        printf("Both numbers are equal.\n");
    }

    return 0;
}
